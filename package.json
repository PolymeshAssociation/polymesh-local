{
  "name": "@polymeshassociation/polymesh-local",
  "description": "Polymesh local is a CLI based tool for managing a Polymesh development environment",
  "version": "5.12.0-alpha.4",
  "author": "Polymesh Association",
  "license": "Apache-2.0",
  "bin": {
    "polymesh-local": "./bin/run"
  },
  "scripts": {
    "generate-snapshot": "ts-node scripts/generate-snapshot",
    "build-uis": "ts-node scripts/build-uis",
    "upload-uis": "ts-node scripts/upload-uis",
    "prepare": "husky install",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "build:ts": "tsc -b && copyfiles -u 1 \"src/local/**/*\" lib",
    "commit": "npx git-cz",
    "semantic-release": "semantic-release",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "test": "jest --detectOpenHandles",
    "format": "cross-env prettier-eslint $PWD\"/src/**/*.{ts,tsx,js,jsx,json,css,md}\" --write"
  },
  "bugs": "https://github.com/PolymeshAssociation/polymesh-local/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/inquirer": "^8.1.3",
    "@types/rimraf": "^3.0.2",
    "cli-ux": "^5.6.3",
    "docker-compose": "0.23.13",
    "inquirer": "^8.2.0",
    "node-fetch": "^2.6.9",
    "rimraf": "^3.0.2",
    "tar": "^6.1.13",
    "tslib": "^1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-modules-commonjs": "7.14.5",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@oclif/dev-cli": "^1",
    "@polkadot/api": "4.16.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.0",
    "@types/cli-progress": "^3.9.2",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.4.0",
    "@types/node-fetch": "^2.6.3",
    "@types/tar": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "babel-jest": "^29.5.0",
    "cli-progress": "^3.9.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-semistandard": "17.0.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-standard": "5.0.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.1",
    "globby": "^10",
    "husky": "7.0.1",
    "jest": "^29.5.0",
    "lint-staged": "11.1.1",
    "npm-run-path": "^4.0.1",
    "prettier": "2.3.2",
    "prettier-eslint": "15.0.1",
    "prettier-eslint-cli": "7.1.0",
    "semantic-release": "21.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "8",
    "typedoc": "0.23.18",
    "typescript": "4.6.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/PolymeshAssociation/polymesh-local",
  "keywords": [
    "oclif"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "polymesh-local",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "PolymeshAssociation/polymesh-local",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "lib/index.d.ts"
}
