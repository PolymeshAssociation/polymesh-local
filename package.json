{
  "name": "polymesh-local",
  "description": "Polymesh local environment for development and e2e testing",
  "version": "1.0.0",
  "author": "Jeremias Diaz @monitz87",
  "bin": {
    "polymesh-local": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "build:ts": "tsc -b && copyfiles -u 1 \"src/public/**/*\" lib",
    "commit": "npx git-cz",
    "semantic-release": "semantic-release",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "cross-env prettier-eslint $PWD\"/src/**/*.{ts,tsx,js,jsx,json,css,md}\" --write"
  },
  "bugs": "https://github.com/PolymathNetwork/polymesh-local/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-ux": "^5.6.3",
    "docker-compose": "0.23.13",
    "node-fetch": "^2.6.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@oclif/dev-cli": "^1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "16.4.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-semistandard": "16.0.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-standard": "5.0.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.1",
    "globby": "^10",
    "husky": "7.0.1",
    "lint-staged": "11.1.1",
    "npm-run-path": "^4.0.1",
    "prettier": "2.3.2",
    "prettier-eslint": "13.0.0",
    "prettier-eslint-cli": "5.0.1",
    "semantic-release": "17.4.4",
    "ts-node": "8",
    "typedoc": "0.16.8",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/PolymathNetwork/polymesh-local",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "polymesh-local",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "PolymathNetwork/polymesh-local",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "types": "lib/index.d.ts"
}
